{
  "setup" : [
    "echo \"notExistingAgentXXX\" > not-existing-enabled-agents.conf && echo \"agents/on-off-agent\" > on-off-agent-enabled-agents.conf "
  ],
  "between_runs" : [ "" ],
  "scenarios" : [
    {
      "name" : "NotExistingAgent",
      "timeout" : 5,
      "arguments" : {
        "--enabled-agents" : "not-existing-enabled-agents.conf"
      }
    },
    {
      "name" : "OnOffAgent",
      "timeout" : 5,
      "arguments" : {
        "--enabled-agents" : "on-off-agent-enabled-agents.conf"
      }
    }
  ],
  "cleanup" : [
    "rm -f *-enabled-agents.conf"
  ]
}
